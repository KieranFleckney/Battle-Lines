{
    "name": "Irregular-Rounded-Line-Generator",
    "version": "1.0.0",
    "description": "Creates abstract images in the form of irregular rounded lines",
    "main": "./dist-cjs/index.js",
    "module": "./dist-esm/index.js",
    "author": "Kieran Fleckney",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "node ./test.js",
        "build": "npm run clean && npm run buildNode && npm run buildEsm && npm run buildBundle",
        "buildEsm": "tsc -m es6 --outDir dist-esm",
        "buildNode": "tsc",
        "buildBundle": "webpack",
        "clean": "shx rm -rf dist dist-esm dist-cjs",
        "buildTest": "npm run build && npm run test"
    },
    "devDependencies": {
        "awesome-typescript-loader": "^5.2.1",
        "shx": "^0.3.2",
        "typescript": "^3.9.5",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {}
}
